<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.mgsystem.user.dao.UserNameDAO">
	<select id="findAll" resultType="cn.tedu.mgsystem.user.entity.UserNameEmp">
		select * from user
	</select>
	<select id="findById" parameterType="String"
		resultType="cn.tedu.mgsystem.user.entity.UserNameEmp">
		select * from user where account=#{user}
	</select>
</mapper> 