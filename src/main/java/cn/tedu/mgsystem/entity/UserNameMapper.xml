<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.tedu.uv.dao.UserNameDAO">
	<select id="findAll" resultType="cn.tedu.uv.entity.UserNameEmp">
		select * from username
	</select>
	<select id="findById" parameterType="String"
		resultType="cn.tedu.uv.entity.UserNameEmp">
		select * from username where user=#{user}
	</select>
</mapper> 